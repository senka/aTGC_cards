#import model loop_sm-ckm_no_b_mass
#switch to diagonal ckm matrix if needed for speed

import model EWdim6NLO-no_cbmass_cwwwcbcw

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-
define V = w+ w-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

generate p p > ell+ vl a QED=3 [QCD] @0
add process p p > ell- vl~ a QED=3 [QCD] @1
add process p p > ell+ vl a j QED=3 [QCD] @2
add process p p > ell- vl~ a j QED=3 [QCD] @3

output WA_aTGC_lep_NLO -nojpeg

